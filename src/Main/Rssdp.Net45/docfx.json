{
	"metadata": [
		{
			"src": [
				{
					"files": [ "**.cs" ],
					"src": "..",
					"exclude": [
						"**/obj/**",
						"**/bin/**",
						"../docs/_site/**",
						"**Bom**",
						"**Test**"
					]
				}
			],
			"dest": "api"

		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml"
				]
			},
			{
				"files": [
					"api/*.md",
					"articles/**.md",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"dest": "../../../docs",
		"globalMetadataFiles": [],
			"globalMetadata": {
				"_appTitle": "RSSDP",
				"_appFaviconPath": "images/RssdpIcon.ico",
				"_appLogoPath": "images/RssdpIconSmall.png",
				"_appFooter": "Copyright (c) 2017 Troy Willmot"
			},
		"fileMetadataFiles": [],
		"template": [ "statictoc", "memberpage/2.24.0/content" ],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false
	}
}